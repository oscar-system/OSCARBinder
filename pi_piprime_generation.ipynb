   "source": [
    "function prove_generation(name, prg, p)\n",
    "  S = atlas_group(name)\n",
    "  P = sylow_subgroup(S, p)[1]\n",
    "  reps = evaluate(prg, gens(S))\n",
    "  good = elem_type(S)[]\n",
    "  for x in filter(!isone, reps)\n",
    "    U = P\n",
    "    g = x\n",
    "    while order(U) < order(S)\n",
    "      g = x^rand(S)\n",
    "      U = sub(S, vcat(gens(P), [g]))[1]\n",
    "    end\n",
    "    push!(good, g)\n",
    "  end\n",
    "  return (P, good)\n",
    "end;"
   ]
  },
